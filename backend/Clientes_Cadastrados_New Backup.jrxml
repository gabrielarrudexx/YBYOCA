	<queryString>
		<![CDATA[SELECT  PAR.CODPARC AS "Nro do Cliente", PAR.AD_NOMFANTASISA As "Desc. Cliente" , CID.NOMECID As "Cidade_1",
        ULT.NUMNOTA  AS "Nro nota"  ,
        ULT.DTNEG AS "Dt.Negoc.",
        ULT.VLRNOTA AS "VLR_Nota_Consid_Devol"
   FROM TGFPAR PAR
   JOIN TSICID CID ON (CID.CODCID = PAR.CODCID)
                        AND (PAR.CLIENTE = 'S')
   LEFT JOIN (SELECT CAB.NUMNOTA, CAB.CODPARC, CAB.CODVEND, CAB.DTNEG, CAB.VLRNOTA  FROM TGFCAB CAB
               WHERE CAB.TIPMOV = 'P'
                 AND CAB.CODTIPOPER IN (34,145,228,169,239,231)
                 AND CAB.HRMOV = (SELECT MAX(CAB2.HRMOV) FROM TGFCAB CAB2 WHERE CAB2.CODPARC = CAB.CODPARC AND CAB2.TIPMOV = 'P'
                                     AND CAB2.CODTIPOPER IN (34,145,228,169,239,231)
                                     AND CAB2.DTNEG = (SELECT MAX(CAB3.DTNEG) FROM TGFCAB CAB3 WHERE CAB3.TIPMOV = 'P'
                                                          AND CAB3.CODTIPOPER IN (34,145,228,169,239,231) AND CAB3.CODPARC = CAB2.CODPARC)))
                     ULT ON (ULT.CODPARC = PAR.CODPARC)

   WHERE (CID.CODCID = NVL($P{pCidade},CID.CODCID) OR CID.CODCID IS NULL  )
     AND (PAR.CODVEND = NVL($P{pVend},PAR.CODVEND))
     AND PAR.ATIVO = 'S'
 ORDER BY PAR.CODPARC]]>
	</queryString>
